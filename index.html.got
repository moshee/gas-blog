<!doctype html public "<!doctype html">
<head>
	<meta charset=utf-8>
	<title>Things and Stuff</title>
	<link rel="stylesheet" href="/assets/blog.css">
</head>
<body>
	<section id=main>
		<header id=header>
			<h1>Things and Stuff</h1>
			<h2>by moshee <a href="https://twitter.com/moshee"><img src="/assets/twitter.svg"></a><a href="https://github.com/moshee"><img src="/assets/github.svg"></a></h2>
		</header>
		{{range .}}<article>
			<h1>{{.Title}}</h1>
			<div class=timestamp>Posted on <time>{{.Timestamp}}</time></div>
			<div class=comment-pointer>{{.Id}}</div>
			<p>{{.Body}}</p>
		</article>{{end}}
		<footer id=footer>wat</footer>
	</section>
</body>
