<!doctype html public "<!doctype html">
<head>
	<meta charset=utf-8>
	<title>Blag</title>
	<link rel="stylesheet" href="/assets/style.css">
</head>
<body>
	<section id=main>
		<header id=header>A blog by moshee about things and stuff.</header>
		{{range .}}<article>
			<h1>{{.Title}}</h1>
			<div class=timestamp>Posted on <time>{{.Timestamp}}</time></div>
			<div class=comment-pointer>{{.Id}}</div>
			<p>{{.Body}}</p>
		</article>{{end}}
		<footer id=footer>wat</footer>
	</section>
</body>
