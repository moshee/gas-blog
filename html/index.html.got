{{template "header"}}
{{range .}}
<article id="post{{.Id}}">
  <time title="{{.Time}}" datetime="{{.Time}}" pubdate>
    <div class=day>{{.Time.Day}} {{.Time.Month | printf "%.3s"}}</div>
    <div class=year>{{.Time.Year}}</div>
  </time>
  <h1>{{.Title}} <a href="/blog/post/{{.Id}}">âˆž</a></h1>
  <p>{{.Body}}</p>
</article>{{end}}
{{template "footer"}}
